{
  "name": "jack-highlighter",
  "displayName": "Jack-Highlighter",
  "description": "Syntax highlighting for Jack VM programming language",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "jack",
        "aliases": [
          "Jack (VM)",
          "jack"
        ],
        "extensions": [
          ".jack"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/Csral/Jack-Highlighter"
    },
    "grammars": [
      {
        "language": "jack",
        "scopeName": "source.jack",
        "path": "./syntaxes/jack.tmLanguage.json"
      }
    ]
  },
  "publisher": "Csral"
}
